<script>
  // apparently there is jQuery in this site, I take no responsibility.

  // basic hover effects
  $('.face-container').on('mouseenter', function(ev) {
    $(ev.currentTarget).addClass('face-container-hover');
  });
  $('.face-container').on('mouseleave', function(ev) {
    $(ev.currentTarget).removeClass('face-container-hover');
  });

  // add hover effects also on click
  $('.face-container').on('tap', function(ev) {
    if( $(ev.currentTarget).hasClass('face-container-hover') ) {
      return $(ev.currentTarget).removeClass('face-container-hover');
    }
    return $(ev.currentTarget).addClass('face-container-hover');
  });

</script>

